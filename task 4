#include <iostream>
using namespace std;

int main() {
    int year, month;

    cout << "Enter a month (1â€“12): ";
    cin >> month;

    cout << "Enter a year: ";
    cin >> year;

    if (month == 1 || month == 3 || month == 5 || month == 7 ||
        month == 8 || month == 10 || month == 12) {
        cout << "31 days" << endl;
    }
    else if (month == 4 || month == 6 || month == 9 || month == 11) {
        cout << "30 days" << endl;
    }
    else if (month == 2) {
        if ((year % 100 == 0 && year % 400 == 0) ||
            (year % 100 != 0 && year % 4 == 0))
            cout << "29 days" << endl;
        else
            cout << "28 days" << endl;
    }
    else {
        cout << "Invalid month entered!" << endl;
    }

    return 0;
}

    


